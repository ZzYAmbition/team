page {
  background-color: #f1f1f1;
}
.header {
  width: 100%;
}
.s {
  display: flex;
  width: 100%;
  height: 39px;
  background-color: #11EE96;
  justify-content: center;
  align-items: center;
  flex-direction: row;
}
.s .sinput {
  width: 160px;
  height: 20px;
  background-color: #fff;
  border-radius: 5px;
}
.usercon {
  margin-top: 10px;
}
.u {
  display: flex;
  width: 100%;
  height: 30px;
  line-height: 30px;
  background-color: #fff;
}
.u .pro {
  width: 30px;
  height: 30px;
  border-radius: 15px;
  vertical-align: middle;
}
.u .upro {
  margin-left: 10px;
}
.u .btn {
  width: 60px;
  height: 30px;
  border-radius: 10px;
  margin-left: 40px;
  background-color: #11EE96;
  color: #fff;
  text-align: center;
}